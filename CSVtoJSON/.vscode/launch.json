{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Deno",
            "type": "pwa-node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--allow-all",
                "--unstable",
                "--inspect-brk",
                "convert.ts",
                "assets",
                "--out",
                "test",
            ],
            "outputCapture": "std",
            "attachSimplePort": 9229
        }
    ]
}